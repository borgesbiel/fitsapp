<% User.not_admin.each do |recipient| %>
  <% conversation = Conversation.between(current_user.id, recipient.id).present? %>
  <% unless conversation.present? %>
    <% if recipient.id != current_user.id %>
      <tr>
        <td><%= recipient.email %></td>
        <td><%= recipient.type %></td>
        <td><%= link_to 'New chat!', conversations_path(sender_id: current_user.id, recipient_id: recipient.id), method: 'post' %></td>
      </tr>
    <% end %>
  <% end %>
<% end %>
